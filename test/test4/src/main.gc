def main() -> int {
    val: int = 2;
    switch (val) {
        case (1) {
            printf("Value is 1.\n");
        } case (2) {
            printf("Value is 2.\n");
        } case (3) {
            printf("Value is 3.\n");
        } other {
            printf("Value is unknown.\n");
        }
    }

    counter: int = 0;
    while (counter < 5) {
        printf("Counter: %i\n", counter);
        counter = counter + 1;
    };
    return 0;
}
