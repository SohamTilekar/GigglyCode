struct Point {
    x: int;
    y: int;
}

struct Rectangle {
    width: int;
    height: int;
}

struct Circle {
    radius: int;
}

struct Triangle {
    base: int;
    height: int;
}

struct Square {
    side: int;
}

struct Polygon {
    sides: array[int];
}
struct Ellipse {
    major_axis: int;
    minor_axis: int;
}
